  "dependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babelify": "^10.0.0",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-browserify": "^0.5.1",
    "gulp-csso": "^4.0.1",
    "gulp-sass": "^4.1.0",
    "morgan": "^1.10.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "pug": "^3.0.0",
    "socket.io": "^3.1.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/server.js --ignore assets/ --ignore src/static",
    "dev:assets": "gulp dev",
    "dev": "yarn dev:assets && yarn dev:server",
    "prebuild": "rm -rf build",
    "build": "yarn build:assets && yarn build:server && yarn copyAll",
    "build:assets": "gulp build",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "prestart": "yarn build",
    "start": "node build/server.js"
  }
}
